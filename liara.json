{
  "platform": "django",
  "django": {
    "pythonVersion": "3.12",
    "collectStatic": false,
    "compileMessages": false,
    "modifySettings": true,
    "timezone": "Asia/Tehran"

  },
  "app": "sinansapi",
  "disks": [
    {
      "name": "staticfiles",
      "mountTo": "staticfiles"
    },
    {
      "name": "media",
      "mountTo": "media"
    }
  ],
  "cron": [
    "python manage.py check_order_time_notif",
    "python manage.py check_order_time"

  ]
}
